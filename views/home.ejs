    <script src="https://unpkg.com/flickity@2/dist/flickity.pkgd.min.js"></script>
    <script>
    let mainCarousel = ".tricks-slider-2";
    let mainSlides = ".tricks-slider_slide-2";
    let parallaxPercentage = 49;
    let flkty = new Flickity(mainCarousel, { contain: true, freeScroll: true, percentPosition: true, pageDots: false, cellSelector: mainSlides, cellAlign: "left", resize: true, selectedAttraction: 0.01, dragThreshold: 1, freeScrollFriction: 0.05 });
    flkty.on("scroll", function (progress) {
      setImagePositions();
      let progressFill = document.querySelector(".progress_fill");
      if (progressFill) { progressFill.style.width = `${progress * 100}%`; }
    });
    function setImagePositions() {
      document.querySelectorAll(mainSlides).forEach((targetElement) => {
        let imageElement = targetElement.querySelector(".image-21"); // Note: image-21 class wasn't in original HTML provided but kept for safety
        if (!imageElement) return;
        let elementOffset = targetElement.offsetLeft + targetElement.offsetWidth - document.querySelector(mainCarousel).offsetLeft;
        let parentWidth = document.querySelector(mainCarousel).offsetWidth + targetElement.offsetWidth;
        let myProgress = elementOffset / parentWidth;
        let slideProgress = parallaxPercentage * myProgress;
        slideProgress = Math.max(0, Math.min(parallaxPercentage, slideProgress));
        imageElement.style.transform = `translateX(-${slideProgress}%)`;
      });
    }
    setImagePositions();
    </script>
